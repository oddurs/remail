{
  "name": "gmail-redesign",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 1122",
    "build": "next build",
    "start": "next start -p 1122",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --project unit",
    "test:components": "vitest run --project components",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:a11y": "playwright test --grep @a11y",
    "test:all": "vitest run && playwright test",
    "test:report": "node tests/report.mjs",
    "seed:generate": "tsx --env-file=.env.local tools/seed/cli.ts generate",
    "seed:reset": "tsx --env-file=.env.local tools/seed/cli.ts reset",
    "seed:preview": "tsx --env-file=.env.local tools/seed/cli.ts preview",
    "seed:stats": "tsx --env-file=.env.local tools/seed/cli.ts stats",
    "seed:clean": "tsx --env-file=.env.local tools/seed/cli.ts clean"
  },
  "dependencies": {
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.563.0",
    "next": "^15.1.0",
    "radix-ui": "^1.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.6.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@eslint/eslintrc": "^3.2.0",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.1.3",
    "eslint": "^9.16.0",
    "eslint-config-next": "^15.1.0",
    "jsdom": "^28.0.0",
    "playwright": "^1.58.2",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
}
